class Solution {
    // Function to return an ArrayList with exact result and formatted result
    static ArrayList<Float> divisionWithPrecision(float a, float b) {
        // code here
        ArrayList<Float> ans=new ArrayList<>();
        float e=a/b;
        ans.add(e);
        float r=Math.round(e*1000f)/1000f;
        ans.add(r);
        return ans;
    }
}
