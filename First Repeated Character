// User function Template for Java
class Solution {
    String firstRepChar(String s) {
        // code here
        HashSet<Character> see=new HashSet<>();
        for(int i=0;i<s.length();i++){
            if(!see.contains(s.charAt(i))){
                see.add(s.charAt(i));
            }
            else{
                return ""+s.charAt(i);
            }
        }
        return "-1";
    }
}
