// User function Template for Java

class Solution {
    boolean check_elements(int arr[], int n, int A, int B) {
        HashSet<Integer> se=new HashSet<>();
        for(int i:arr){
            se.add(i);
        }
        for(int i=A;i<=B;i++){
            if(!se.contains(i)){
                return false;
            }
        }
        return true;
    }
}
