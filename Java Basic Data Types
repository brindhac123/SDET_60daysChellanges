class Solution {

    int javaIntType(Scanner sc) {
        // code here
        int a=sc.nextInt();
        return a;
    }

    String javaStringType(Scanner sc) {
        String b=sc.next();
        return b;
        // code here
    }

        
    float javaFloatType(Scanner sc) {
        // code here
        float c=sc.nextFloat();
        return c;
    }
};
