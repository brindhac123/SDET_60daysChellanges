import java.util.*;

public class Solution {
    public static void main(String args[]) {
        // Your Code Here
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int c=0;
        for(int i=2;i<=n/2;i++){
            if(n%i==0){
                c+=1;
            }
        }
        if(c==0)
        System.out.println("True");
        else
        System.out.println("False");
    }
}
