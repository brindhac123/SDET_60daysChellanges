import java.util.*;

public class Solution {
    public static void solve() {
        // Your Code Here
        Scanner s=new Scanner(System.in);
        
        int n=s.nextInt();
        if(n>100){
            System.out.println("Big");
        }else if(n<10){
            System.out.println("Small");
        }else{
            System.out.println("Number");
        }
    }
}
