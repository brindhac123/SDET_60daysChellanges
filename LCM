// User function Template for Java
class Solution {
    public static int LCM(int a, int b) {

        // write your code here
        int x = a, y = b; // store original values

        // calculate GCD
        while (b != 0) {
            int temp = b;
            b = a % b;
            a = temp;
        }

        int gcd = a;
        return (x / gcd) * y; // LCM formula

        // return LCM of a and b
    }
}
