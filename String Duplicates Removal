// User function Template for Java

class Solution {
    String removeDuplicates(String s) {
        HashSet<Character> st=new HashSet<>();
        StringBuilder sb=new StringBuilder();
        for(int i=0;i<s.length();i++){
            char c=s.charAt(i);
            if(!st.contains(c)){
                st.add(c);
                sb.append(c);
            }
        }
        return sb.toString();
    }
}
