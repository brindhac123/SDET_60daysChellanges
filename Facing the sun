// User function Template for Java

class Solution {
    // Returns count buildings that can see sunlight
    public int countBuildings(int[] height) {
        // code here
        int n=height.length,max=height[0],c=1;
        for(int i=1;i<n;i++){
            if(height[i]>max){
                c+=1;
                max=height[i];
            }
        }return c;
    }
}
