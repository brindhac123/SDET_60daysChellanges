class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int n=arr.length,ind=0;
        for(int i=0;i<n;i++){
            if(arr[i]!=0){
                arr[ind]=arr[i];
                ind++;
            }
        }
        while(ind<n){
            arr[ind]=0;
            ind++;
        }
    }
}
