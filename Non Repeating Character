class Solution {
    public char nonRepeatingChar(String s) {
        // code here
        int ar[]=new int[26];
        for(int i=0;i<s.length();i++){
            ar[s.charAt(i)-'a']++;
        }
        for(int i=0;i<s.length();i++){
            if(ar[s.charAt(i)-'a']==1){
                return s.charAt(i);
            }
        }
        return '$';
    }
}
